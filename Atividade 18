using System;

class Program
{
  static void Main()
  {
      int [] vetor = new int[5];
      
      Console.Write("Digite os n√∫meros do vetor: ");
      for (int i = 0; i < vetor.Length; i++){
       vetor[i] = int.Parse(Console.ReadLine());
        }
      Array.Reverse(vetor);
    
    Console.WriteLine("Vetor em ordem decrescente: ");
    for (int i = 0; i < vetor.Length; i++){
    Console.WriteLine(vetor[i]);
      }
  }
}
