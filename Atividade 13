using System;

class Program
{
    static void Main()
    {
        double[] numeros = { 10.5, 20.3, 15.8, 30.2, 25.7 };
        double soma = 0;

        for (int i = 0; i < numeros.Length; i++)
        {
            soma += numeros[i];
        }

        double media = soma / numeros.Length;

        Console.WriteLine($"A média dos valores é: {media:F2}");
    }
}
